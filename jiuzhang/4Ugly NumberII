class Solution {
public:
    /*
     * @param n an integer
     * @return the nth prime number as description.
     */
    int nthUglyNumber(int n) {
        // write your code here
        if (n=1){
            return 1;
        }
        
        int num = 2
        n--;
        while (n>0){
            if (!(n%2) && !(n%3) && !(n))
        }
    }
};
